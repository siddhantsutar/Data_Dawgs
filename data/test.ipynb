    "import pandas as pd\n",
    "import sqlite3\n",
    "\n",
    "# Read sqlite query results into a pandas DataFrame\n",
    "con = sqlite3.connect(\"node5.db\")\n",
    "df = pd.read_sql_query(\"SELECT * from movies\", con)\n",
    "\n",
    "dummies = pd.get_dummies(df['writer'])\n",
    "\n",
    "print(dummies)\n",
    "\n",
    "con.close()"
   ]
